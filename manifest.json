{
    "manifest_version": 3,
    "name": "Simple Twitch",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "http://www.twitch.tv/*",
                "https://www.twitch.tv/*"
            ],
            "css": ["content.css"],
            "js": ["content.js"]
        }
    ]
}